    "about_intro": {
      "type": "simple-section",
      "blocks": {
        "statement-1": {
          "type": "image-and-text",
          "settings": {
            "image": "shopify://shop_images/PIERCINGROOM2_1699557753-1_jpeg_7fedd188-7b8b-47c4-a353-a516c3c02655.webp",
            "image_alt": "",
            "direction": "image-left"
          },
          "blocks": {
            "statement-text-1": {
              "type": "text",
              "settings": {
                "text": "<h4>A space made for you</h4><p>At Hone, we want every piercing to feel entirely your own. From the moment you walk in, our team is here to guide you with care, expertise, and zero pressure—helping you discover pieces and placements that reflect your style.</p>",
                "text_style": "base",
                "alignment": "left"
              },
              "blocks": {}
            }
          },
          "block_order": [
            "statement-text-1"
          ]
        },
        "statement-2": {
          "type": "image-and-text",
          "settings": {
            "image": "shopify://shop_images/HoneAbout.jpg",
            "image_alt": "",
            "direction": "image-right"
          },
          "blocks": {
            "statement-text-2": {
              "type": "text",
              "settings": {
                "text": "<h4>Contemporary style with a personal touch</h4><p>Our studio is designed with comfort and ease in mind. Every detail, from our sterilization processes to our carefully selected jewelry, reflects our commitment to safety and precision.</p>",
                "text_style": "base",
                "alignment": "left"
              },
              "blocks": {}
            }
          },
          "block_order": [
            "statement-text-2"
          ]
        }
      },
